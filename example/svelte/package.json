{
  "name": "svelte",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview  --port 3000",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:e2e": "PROJECT_ROOT=$(pwd) npm run test:e2e --prefix node_modules/@progressively/cypress-helpers",
    "test:e2e:ci": "PROJECT_ROOT=$(pwd) npm run test:e2e:ci --prefix node_modules/@progressively/cypress-helpers"
  },
  "dependencies": {
    "@progressively/sdk-js": "0.0.1-alpha.19",
    "@progressively/server-side": "0.0.1-alpha.19"
  },
  "devDependencies": {
    "@progressively/cypress-helpers": "1.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "cypress": "^9.6.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module"
}
