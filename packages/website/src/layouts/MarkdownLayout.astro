---
import Container from "../components/Container.astro";
import Layout from "./Layout.astro";

export interface Props {
  content: { title: string };
}

const { content } = Astro.props;
const { title } = content;
---

<Layout title={title}>
  <Container>
    <main class="prose prose-slate py-12">
      <slot />
    </main>
  </Container>
</Layout>
