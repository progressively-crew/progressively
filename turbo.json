{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": ["build"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "start:frontend": {
      "dependsOn": ["^build"]
    },
    "start:backend": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": []
    },
    "db:migrate:dev": {
      "dependsOn": ["^build"]
    },
    "db:migrate:deploy": {
      "dependsOn": ["^build"]
    },
    "db:seed": {
      "dependsOn": []
    },
    "db:cleanup": {
      "dependsOn": []
    },
    "bundlesize": {
      "dependsOn": ["build"]
    }
  }
}
