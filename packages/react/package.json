{
  "name": "@progressively/react",
  "author": "mfrachet",
  "version": "0.0.1-beta.1",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.mjs"
    }
  },
  "types": "./lib/cjs/types/src/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "build:dev": "pnpm run build -- --environment BUILD:development",
    "test": "CI=true vitest",
    "test:watch": "vitest",
    "bundlesize": "bundlesize --enable-github-checks"
  },
  "peerDependencies": {
    "react": ">= 17"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^18.0.28",
    "bundlesize2": "^0.0.31",
    "msw": "^1.0.1",
    "node-fetch": "2.6.7",
    "react": ">=18",
    "react-dom": ">=18",
    "rollup": "^3.15.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "@progressively/sdk-js": "workspace:0.0.1-alpha.19"
  },
  "bundlesize": [
    {
      "path": "./lib/esm/index.mjs",
      "maxSize": "1.3 kB"
    },
    {
      "path": "./lib/cjs/index.cjs.js",
      "maxSize": "1.3 kB"
    }
  ]
}
