#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marvin/soft/progressively/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/marvin/soft/progressively/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/react-native@0.71.3_react@18.2.0/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/react-native@0.71.3_react@18.2.0/node_modules/react-native/cli.js" "$@"
fi
