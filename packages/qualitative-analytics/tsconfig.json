{
  "compilerOptions": {
    "target": "es5", // For older browsers, use "es5"
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "allowJs": true,
  },
  "include": ["src/**/*"]
}