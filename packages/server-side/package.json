    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^28.1.1",
    "@types/node-fetch": "^2.6.1",
    "bundlesize2": "^0.0.31",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "msw": "^0.42.1",
    "rollup": "^2.75.6",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3"
  },
  "bundlesize": [
    {
      "path": "./lib/esm/index.js",
